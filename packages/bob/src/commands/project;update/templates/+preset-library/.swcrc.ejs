{
  "$schema": "http://json.schemastore.org/swcrc",
  "module": {
    "type": "es6"
  },
  "jsc": {
    "target": "esnext",
    "baseUrl": "./src",
    "parser": {
      "syntax": "typescript",
      "tsx": true
    },
    "paths": {
      "~/*": ["./*"]
    },
    "transform": {
      "react": {
        "runtime": "automatic"
      }
    }
  }
}
